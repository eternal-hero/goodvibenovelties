@import url('https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,100;0,300;0,400;0,700;0,900;1,100;1,300;1,400;1,700;1,900&display=swap');
body{background-color: #fff !important; margin:0; padding:0;}
body ,h1,h2,h3,h4,h5,h6,p,span, a{font-family: 'Lato', sans-serif !important;}
.button:not([disabled]):hover:after, .shopify-challenge__button:hover:after, .customer button:hover:after, .shopify-payment-button__button--unbranded:hover:after {
    --border-offset: 0;
    box-shadow: none;
}span.prod-discount {
    font-size: 1.3rem;
    line-height: calc(1 + .5 / var(--font-body-scale));
    letter-spacing: .04rem;
}
.vendor_img {
    min-height: 10vw;
    display: inline-block;
    width: 100%;
    text-align: center;
}
.vendor-list li img {
    width: 100%;
}
.topvendor ul li img {
    width: 100%;
}
.topvendor ul {
    display: grid;
    grid-template-columns: repeat(5,1fr);
    margin-left: 0;
    padding-left: 0;
    column-gap: 25px;
}
.topvendor ul li {
    list-style: none;
    text-align: center;
}
.topvendor ul a {
    text-decoration: none;
    color: #000;
}
.vendor-list h3 {
    margin-bottom: 10px;
    font-weight: 800;
    background: #ebebeb;
    padding: 13px;
    margin-top: 50px;
    margin-bottom: 50px;
}
header ul.list-menu li:last-child a span {
    text-decoration: none !important;
}
.vendor-list ul {
    margin-left: 0;
    padding-left: 0px;
}
.vendor-list li a {
    color: #000;
    text-decoration: none;
}
.vendor-list li {
    margin-left: 0;
    list-style: none;
    width: 24.6%;
    display: inline-block;
    padding: 0 2%;
    text-align: center;
    margin-bottom: 40px;
}
header ul.list-menu li:last-child a {
    margin-top: 13px;
    text-decoration: none !important;
    border: 0 !important;
    margin-left: 12px;
    background:none !important;
}
.product-form__quantity .qtybtn {
    display: flex;
    align-items: flex-start;
}

details-modal.header__search {
    display: none !important;
}

.sale-promo-banner {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background-color: #e60060;
    z-index: 9;
    opacity: 0;
  visibility: hidden;
      transition: .2s;
}

.sale-promo-banner .inner-eight--logos {
    padding: 0 15px;
    display: flex;
    align-items: center;
    justify-content: center;
    max-width: 1080px;
    margin: auto;
}

.sale-promo-banner .inner-eight--logos .inner-logo-text {
    width: 33.3%;
    position: relative;
    padding: 0 10px;
}

.sale-promo-banner .inner-logo-text:after {
    content: " ";
    position: absolute;
    right: 3px;
    top: 50%;
    transform: translateY(-50%);
    width: 5px;
    height: 5px;
    border-radius: 10px;
    background: #fff;
}

.sale-promo-banner .inner-logo-text:last-child:after{
  display: none;
}

.sale-promo-banner.sale-promo-show {
    opacity: 1;
  visibility: visible;
}

.sale-promo-banner .inner-logo-text p {
    color: #fff;
    font-size: 16px;
    text-align: center;
    line-height: 1;
    font-weight: 600;
}
.you-save  span.prod-discount {
    margin-bottom: 17px !important;
    display: inline-block;
    margin-top: 10px;
}
.product_page .product__tax {
    margin-top: 10px;
}
.product__title.title-on-mobo h2.mobile_ptitle {
    font-size: 22px;
    color: #000;
  letter-spacing: 0em;
  text-align: left;
}

span.compare_price {
    display: flex;
    align-items: center;
}

span.compare_price span.prod-discount {
    margin: 10px 0 10px!important;
}


@media (max-width: 1100px){
nav.header__inline-menu ul.list-menu.list-menu--inline li {
    padding: 0 7px !important;
}
}
@media (max-width: 1024px)
{
  .sku-n-price.on-mobile .product__tax.caption.rte {
    display: none;
}
}



@media (max-width: 989px){
  
  button.search__button.field__button {
    padding: 14px 15px  !important;
    -webkit-border-top-right-radius: 26px;
    -webkit-border-bottom-right-radius: 26px;
    -moz-border-radius-topright: 26px;
    -moz-border-radius-bottomright: 26px;
    border-top-right-radius: 26px !important;
    border-bottom-right-radius: 26px !important;
    border-radius: 0 4px 4px 0;
}
  .sale-promo-banner .inner-logo-text p {
    font-size: 10px;
}
header ul.list-menu li:last-child a {
    margin-top: 0;
    margin-left: 0;
    font-weight: 400 !important;
    padding: 10px 23px !important;

}  
details-modal.header__search {
    display: inline-block !important;
    margin-top: 6px;
}
.wishlist-icon-text {
    padding-right: 5px !important;
}  
}


@media (max-width: 960px){
.vendor-list li {
    width: 32.5%;
}
}

@media (max-width: 767px){
  .you-save span.prod-discount {
    margin-bottom: 0!important;
  
}
  .product__info-wrapper .stock-msg p {
    margin: 0;
}
  details-modal.header__search {
    display: inline-block !important;
    margin-top: 6px;
}
.wishlist-icon-text {
    padding-right: 5px;
}
.sale-promo-banner .inner-eight--logos .inner-logo-text {
    width: 100%;
}

  .sale-promo-banner .inner-eight--logos {
    flex-wrap: wrap;
}

  .sale-promo-banner .inner-logo-text:after{
    display: none;
  }

    .sale-promo-banner .inner-logo-text p {
    margin: 5px 0;
}
  .wishlist-icon-text {
    padding-right: 0 !Important;
}
summary.header__icon.header__icon--search.header__icon--summary {
    width: 3.5rem !Important;
}
}


@media (max-width: 600px){
.vendor-list li {
    width: 49%;
}
 .topvendor ul {
    grid-template-columns: repeat(3,1fr);
} 
}

@media (max-width: 480px){
.wishlist-icon-text {
    padding-right: 0 !Important;
}
summary.header__icon.header__icon--search.header__icon--summary {
    width: 3.5rem !Important;
}
.topvendor ul {
    grid-template-columns: repeat(2,1fr);
}  
}